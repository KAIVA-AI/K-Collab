{
    "compilerOptions": {
     "module": "commonjs",
		"target": "ES2020",
		"lib": [
			"DOM", 
			"ES2020"
		],
      "jsx": "react",
      // "module": "esnext",
      "moduleResolution": "node",
      "allowJs": true,
      "outDir": "./dist",
      "sourceMap": true,
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }