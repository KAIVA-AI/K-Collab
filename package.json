{
  "name": "k-collab",
  "displayName": "K-Collab",
  "description": "VIETIS AI powered collaborative chat for developers",
  "version": "1.0.0",
  "license": "MIT",
  "repository": "https://github.com/KAIVA-AI/K-Collab.git",
  "publisher": "protorX",
  "engines": {
    "vscode": "^1.94.0"
  },
  "packageManager": "yarn@1.22.22",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "icon": "static/icon.png",
  "categories": [
    "Other"
  ],
  "keywords": [
    "chat"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onLanguage",
    "onUri"
  ],
  "main": "./dist/vscode/extension.js",
  "scripts": {
    "lint": "eslint packages",
    "start-webview": "yarn workspace @k-collab/webview start",
    "start-vscode": "yarn workspace @k-collab/vscode start",
    "build-webview": "yarn workspace @k-collab/webview build",
    "build-vscode": "yarn workspace @k-collab/vscode build",
    "prepackage": "yarn build-vscode",
    "package": "vsce package"
  },
  "contributes": {
    "handlers": {
      "authentication": {
        "sessions": [
          {
            "id": "k-collab",
            "label": "k-collab",
            "scheme": "k-collab"
          }
        ]
      },
      "uri": [
        {
          "scheme": "vscode",
          "command": "k-collab.handleUri"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "k-collab_bar",
          "title": "K-collab",
          "icon": "static/icon_sidebar.svg"
        }
      ]
    },
    "views": {
      "k-collab_bar": [
        {
          "id": "k-collab_bar.chat",
          "type": "webview",
          "name": "Chat"
        }
      ]
    },
    "icons": {
      "k-collab-icon": {
        "description": "k-collab-icon",
        "default": {
          "fontPath": "./static/icon_themes.woff",
          "fontCharacter": "\\e900"
        }
      }
    },
    "commands": [
      {
        "command": "k-collab.command.add-selection",
        "title": "Add Selection to Chat",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.add-file",
        "title": "Add File to Chat",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.add-image",
        "title": "Add Image to Chat",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.coding.gen-code",
        "title": "Generate Code",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.coding.gen-test",
        "title": "Generate Test",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.coding.gen-docs",
        "title": "Generate Docs",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.coding.comment",
        "title": "Comment",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.coding.porting",
        "title": "Porting",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.coding.explain",
        "title": "Explain",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.coding.improve",
        "title": "Improve",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.coding.review",
        "title": "Review",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.coding.fixbug",
        "title": "Fixbug",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.coding.html",
        "title": "HTML",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.coding.html_item",
        "title": "HTML Item",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.coding.db_create_table",
        "title": "Db Create Table",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.coding.db_create_query",
        "title": "DB Create Query",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.coding.db_modify_table",
        "title": "DB Modify Table",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.coding.db_modify_query",
        "title": "DB Modify Query",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.coding.db_optimize_query",
        "title": "DB Optimize Query",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.coding.db_fix_query",
        "title": "DB Fix Query",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.coding.db_format_query",
        "title": "DB Format Query",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.coding.db_explain_query",
        "title": "DB Explain Query",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.coding.db_generate_data",
        "title": "DB Generate Data",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.chat.new-topic",
        "title": "New topic",
        "icon": "$(add)",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.chat.history",
        "title": "History",
        "icon": "$(history)",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.chat.preview",
        "title": "Preview",
        "icon": "static/icon_preview_dark.svg",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.inline.chat",
        "title": "Ask VietIS-Coding",
        "category": "K-Collab",
        "enablement": "!commentIsEmpty"
      },
      {
        "command": "k-collab.command.ask-ai",
        "title": "Ask VietIS-Coding",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.preview.accept",
        "title": "Accept",
        "icon": "$(check)",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.preview.reject",
        "title": "Reject",
        "icon": "$(close)",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.setting.open",
        "title": "K-Collab Setting",
        "icon": "$(gear)",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.setting.change-workspace",
        "title": "Change workspace",
        "icon": "$(account)",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.setting.setting",
        "title": "setting",
        "icon": "$(gear)",
        "category": "K-Collab"
      },
      {
        "command": "k-collab.command.setting.logout",
        "title": "Logout",
        "icon": "$(log-out)",
        "category": "K-Collab"
      }
    ],
    "submenus": [
      {
        "id": "k-collab.submenu.editor",
        "label": "K-Collab"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "submenu": "k-collab.submenu.editor",
          "when": "resourceScheme != output",
          "group": "2_k-collab"
        }
      ],
      "k-collab.submenu.editor": [
        {
          "command": "k-collab.command.add-selection",
          "when": "editorHasSelection",
          "group": "1_add@1"
        },
        {
          "command": "k-collab.command.add-file",
          "group": "1_add@2"
        },
        {
          "command": "k-collab.command.coding.explain",
          "group": "2_code@1"
        },
        {
          "command": "k-collab.command.coding.improve",
          "group": "2_code@2"
        },
        {
          "command": "k-collab.command.coding.review",
          "group": "2_code@3"
        },
        {
          "command": "k-collab.command.coding.fixbug",
          "group": "2-code@4"
        },
        {
          "command": "k-collab.command.coding.gen-code",
          "group": "2_code@5"
        },
        {
          "command": "k-collab.command.coding.gen-test",
          "group": "2_code@6"
        },
        {
          "command": "k-collab.command.coding.comment",
          "group": "2_code@7"
        },
        {
          "command": "k-collab.command.coding.porting",
          "group": "2_code@8"
        },
        {
          "command": "k-collab.command.coding.html",
          "group": "3_code@1"
        },
        {
          "command": "k-collab.command.coding.html_item",
          "group": "3_code@2"
        },
        {
          "command": "k-collab.command.coding.db_create_table",
          "group": "4_code@1"
        },
        {
          "command": "k-collab.command.coding.db_create_query",
          "group": "4_code@2"
        },
        {
          "command": "k-collab.command.coding.db_modify_table",
          "group": "4_code@3"
        },
        {
          "command": "k-collab.command.coding.db_modify_query",
          "group": "4_code@4"
        },
        {
          "command": "k-collab.command.coding.db_optimize_query",
          "group": "4_code@5"
        },
        {
          "command": "k-collab.command.coding.db_fix_query",
          "group": "4_code@6"
        },
        {
          "command": "k-collab.command.coding.db_format_query",
          "group": "4_code@7"
        },
        {
          "command": "k-collab.command.coding.db_explain_query",
          "group": "4_code@8"
        },
        {
          "command": "k-collab.command.coding.db_generate_data",
          "group": "4_code@9"
        },
        {
          "command": "k-collab.command.ask-ai",
          "group": "5_ask@1"
        }
      ],
      "view/title": [
        {
          "command": "k-collab.command.chat.new-topic",
          "when": "view == k-collab_bar.chat && (currentWebviewPage == 'chat-page' || currentWebviewPage == 'topic-page')",
          "group": "navigation@1"
        },
        {
          "command": "k-collab.command.chat.history",
          "when": "view == k-collab_bar.chat && currentWebviewPage == 'chat-page'",
          "group": "navigation@2"
        },
        {
          "command": "k-collab.command.chat.preview",
          "when": "view == k-collab_bar.chat && currentWebviewPage == 'chat-page'",
          "group": "navigation@3"
        },
        {
          "command": "k-collab.command.setting.open",
          "when": "view == k-collab_bar.chat",
          "group": "navigation@4"
        }
      ],
      "comments/commentThread/context": [
        {
          "command": "k-collab.command.inline.chat",
          "group": "inline",
          "when": "commentController == k-collab.comment.ask"
        }
      ],
      "editor/title": [
        {
          "command": "k-collab.command.preview.accept",
          "group": "navigation@1",
          "when": "isInDiffEditor && resourceScheme == 'k-collab-memory'"
        },
        {
          "command": "k-collab.command.preview.reject",
          "group": "navigation@2",
          "when": "isInDiffEditor && resourceScheme == 'k-collab-memory'"
        }
      ]
    }
  },
  "author": "namdtavailable <163511609+namdtavailable@users.noreply.github.com>"
}
