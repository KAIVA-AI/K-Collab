{
  "name": "v-collab",
  "displayName": "VCollab",
  "description": "VIETIS AI powered collaborative chat for developers",
  "version": "1.0.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://repo.vietis.com.vn:8009/idt/v-collab-ide-ext"
  },
  "publisher": "vietis",
  "engines": {
    "vscode": "^1.94.0"
  },
  "packageManager": "yarn@1.22.22",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "icon": "static/icon.png",
  "categories": [
    "Other"
  ],
  "keywords": [
    "chat"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onLanguage",
    "onUri"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "yarn compile",
    "compile": "webpack --mode production --devtool hidden-source-map",
    "watch": "webpack --watch",
    "package": "vsce package",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn compile-tests && yarn compile && yarn lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "start-webview": "yarn workspace @v-collab/webview start",
    "start-wv": "yarn start-webview",
    "build-webview": "yarn workspace @v-collab/webview build",
    "build-wv": "yarn build-webview"
  },
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "v-collab_bar",
          "title": "VCollab",
          "icon": "static/icon_sidebar.svg"
        }
      ]
    },
    "views": {
      "v-collab_bar": [
        {
          "id": "v-collab_bar.chat",
          "type": "webview",
          "name": "Chat"
        }
      ]
    },
    "commands": [
      {
        "command": "v-collab.command.ask-ai",
        "title": "Ask VietIS-Coding",
        "category": "VCollab"
      },
      {
        "command": "v-collab.command.add-selection",
        "title": "Add Selection to Chat",
        "category": "VCollab"
      },
      {
        "command": "v-collab.command.add-file",
        "title": "Add File to Chat",
        "category": "VCollab"
      },
      {
        "command": "v-collab.command.explain",
        "title": "Explain",
        "category": "VCollab"
      },
      {
        "command": "v-collab.command.improve",
        "title": "Improve",
        "category": "VCollab"
      },
      {
        "command": "v-collab.command.enhance",
        "title": "Enhance",
        "category": "VCollab"
      },
      {
        "command": "v-collab.command.review",
        "title": "Review",
        "category": "VCollab"
      },
      {
        "command": "v-collab.command.generate-docs",
        "title": "Generate Docs",
        "category": "VCollab"
      },
      {
        "command": "v-collab.command.generate-tests",
        "title": "Generate Tests",
        "category": "VCollab"
      },
      {
        "command": "v-collab.command.chat.new-topic",
        "title": "New topic",
        "icon": "$(add)",
        "category": "VCollab"
      },
      {
        "command": "v-collab.command.chat.history",
        "title": "History",
        "icon": "$(history)",
        "category": "VCollab"
      },
      {
        "command": "v-collab.command.chat.preview",
        "title": "Preview",
        "icon": "static/icon_preview_dark.svg",
        "category": "VCollab"
      },
      {
        "command": "v-collab.command.chat.close",
        "title": "Close",
        "icon": "$(close)",
        "category": "VCollab"
      },
      {
        "command": "v-collab.command.inline.chat",
        "title": "Ask VietIS-Coding",
        "category": "VCollab",
				"enablement": "!commentIsEmpty"
      }
    ],
    "submenus": [
      {
        "id": "v-collab.submenu.editor",
        "label": "VCollab"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "submenu": "v-collab.submenu.editor",
          "group": "2_v-collab"
        }
      ],
      "v-collab.submenu.editor": [
        {
          "command": "v-collab.command.ask-ai",
          "when": "editorHasSelection",
          "group": "1_ask@1"
        },
        {
          "command": "v-collab.command.add-selection",
          "when": "editorHasSelection",
          "group": "2_add@1"
        },
        {
          "command": "v-collab.command.add-file",
          "group": "2_add@2"
        },
        {
          "command": "v-collab.command.explain",
          "group": "3_ask@1"
        },
        {
          "command": "v-collab.command.improve",
          "group": "3_ask@2"
        },
        {
          "command": "v-collab.command.review",
          "group": "3_ask@4"
        }
      ],
      "view/title": [
        {
          "command": "v-collab.command.chat.new-topic",
          "when": "view == v-collab_bar.chat",
          "group": "navigation@1"
        },
        {
          "command": "v-collab.command.chat.history",
          "when": "view == v-collab_bar.chat",
          "group": "navigation@2"
        },
        {
          "command": "v-collab.command.chat.preview",
          "when": "view == v-collab_bar.chat",
          "group": "navigation@3"
        },
        {
          "command": "v-collab.command.chat.close",
          "when": "view == v-collab_bar.chat",
          "group": "navigation@4"
        }
      ],
      "comments/commentThread/context": [
        {
          "command": "v-collab.command.inline.chat",
          "group": "inline",
          "when": "commentController == comment-sample"
        }
      ]
    }
  }
}
